{
    "name": "Inject MV3 (Content Script)",
    "version": "0.01",
    "description": "-",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ],
            "css": [
                "contentScript.css"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "manifest_version": 3
}